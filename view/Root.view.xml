<?xml version="1.0" encoding="UTF-8" ?>
<core:View
	controllerName="com.yo.testapp.view.Root"
	displayBlock="true"
	resourceBundleName="com.yo.testapp.i18n.messageBundle"
	resourceBundleAlias="i18n"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">

	<Shell showLogout="false">
		<SplitApp id="idApp">
			<masterPages>
				<mvc:XMLView viewName="com.yo.testapp.view.Master" id="idViewMaster" />
			</masterPages>
			<detailPages>
				<mvc:XMLView viewName="com.yo.testapp.view.Empty" id="idViewEmpty" />
				<mvc:XMLView viewName="com.yo.testapp.view.Detail" id="idViewDetail" />
				<mvc:XMLView viewName="com.yo.testapp.view.LineItem" id="idViewLineItem" />
			</detailPages>
		</SplitApp>
	</Shell>
	
</core:View>
